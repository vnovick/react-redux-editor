{
  // I want to use babel-eslint for parsing!
  "parser": "babel-eslint",
  "ecmaFeatures": { "regexUFlag": true, "modules": true }
, "env": {
  // I write for browser
  "browser": true
  // in CommonJS
, "node": true
  // I am from the future
, "es6": true
}
  // http://eslint.org/docs/rules/
, "rules": {
  "strict": [0, "global"]
, "quotes": [0, "single"]
, "eol-last": [0]
, "comma-dangle": [0]
, "no-mixed-requires": [0]
, "no-underscore-dangle": [0]
, "no-unused-vars": [1, { "args": "none" }]
, "no-unused-expressions": [0]
, "semi": [1]
, "no-trailing-spaces": [1]
, "camelcase": [1]
, "dot-notation": [1, { "allowKeywords": true }]
, "consistent-return": [2]
, "no-return-assign": [0]
, "no-empty": [1]
, "no-shadow": [1]
, "no-multi-spaces": [1]
, "comma-spacing": [1, { "before": false, "after": true }]
, "no-extra-boolean-cast": [1]
, "no-extra-parens": [1]
, "no-extra-semi": [1]
, "no-func-assign": [2]
, "no-inner-declarations": [2, "both"]
, "no-invalid-regexp": [2]
, "no-irregular-whitespace": [2]
, "valid-jsdoc": [2]
, "indent": [1, 4]
, "radix": [2]
}
}
